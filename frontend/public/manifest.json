{
  "name": "Brevity",
  "description": "Brevity app",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "permissions": [
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://noahsmiths.github.io/*"],
      "js": ["gmail-oauth.js"]
    }
  ],
  "icons": {}
}